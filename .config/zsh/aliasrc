# Default to neovim
alias vi=nvim
alias vim=nvim

# Colorized outputs
alias ls="ls -al -G"

# aws
alias aws="$(dirname $(nvm which current))/aws"

# k8s
alias k=kubectl

# terraform
alias tf=terraform
alias tg="$(dirname $(nvm which current))/terragrunt"
alias terragrunt="$(dirname $(nvm which current))/terragrunt"

# git
alias g=git
